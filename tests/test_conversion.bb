; Test Conversion Functions
i% = 10
f# = 3.14
s$ = "A"

Print "Int(3.14) = " + Int(f#)
Print "Float(10) = " + Float(i%)
Print "Str(10) = " + Str(i%)

Print "Hex(255) = " + Hex(255)
Print "Bin(5) = " + Bin(5)

Print "Asc('A') = " + Asc(s$)
Print "Chr(65) = " + Chr(65)
Print "Len('Hello') = " + Len("Hello")
